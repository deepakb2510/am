<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#00CCCCCC" />
            <stroke
                android:width="2dp"
                android:color="#094985" />
            <corners
                android:radius="50dp" />
            <padding
                android:left="1dp"
                android:top="1dp"
                android:right="1dp"
                android:bottom="1dp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#4a94d9"
                android:endColor="#4a94d9"
                android:angle="270" />
            <stroke
                android:width="2dp"
                android:color="#094985" />
            <corners
                android:radius="50dp" />
            <padding
                android:left="1dp"
                android:top="1dp"
                android:right="1dp"
                android:bottom="1dp" />
        </shape>
    </item>
</selector>